{
  "name": "jnt-copy",
  "version": "0.2.1",
  "description": "API that emulates Job and Talent",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc --build",
    "test": "jest --watchAll --verbose --coverage --collectCoverageFrom=\"./src/**\"",
    "test:prod": "jest --detect-open-handles",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check .",
    "prettier:write": "npx prettier --write .",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js -d src/infrastructure/persistance/datasource migration:generate",
    "migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js -d src/infrastructure/persistance/datasource migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DomingoCast/jnt-copy.git"
  },
  "author": "Hector Plinio, Aitor Segura, Domingo Castillo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DomingoCast/jnt-copy/issues"
  },
  "homepage": "https://github.com/DomingoCast/jnt-copy#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "awilix": "^7.0.1",
    "awilix-express": "^7.0.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sendgrid": "^5.2.3",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
